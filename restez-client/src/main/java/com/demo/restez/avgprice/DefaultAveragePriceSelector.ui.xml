<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
		.priceList{
			list-style-type: none;	
		}
		.priceList li{
			float: left;	
		}
	</ui:style>
	<g:HTMLPanel>
		<gwtbootstrap:Row>
			<gwtbootstrap:Column size="12">
				<g:HTMLPanel>
					<ul class="{style.priceList}">
						<li>
							<gwtbootstrap:Button type="DANGER" text="$" ui:field="lowBtn" />
						</li>
						<li>
							<gwtbootstrap:Button type="WARNING" text="$$" ui:field="medBtn" />
						</li>
						<li>
							<gwtbootstrap:Button type="SUCCESS" text="$$$" ui:field="highBtn" />			
						</li>
					</ul>
				</g:HTMLPanel>				
			</gwtbootstrap:Column>
		</gwtbootstrap:Row>
	</g:HTMLPanel>
</ui:UiBinder> 